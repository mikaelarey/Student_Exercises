@model testing.ViewModel.AdminIndexViewModel
@{
    ViewBag.Title = "View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>

<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">ID</th>
            <th scope="col">First Name</th>
            <th scope="col">Last Name</th>
            <th scope="col">User Account Id</th>
        </tr>
    </thead>
    <tbody>
            @foreach (var item in Model.students)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.FirstName</td>
                    <td>@item.FirstName</td>
                    <td>@item.UserAccountId</td>
                </tr>
            }

    </tbody>
</table>

<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">ID</th>
            <th scope="col">First Name</th>
            <th scope="col">Last Name</th>
            <th scope="col">User Account Id</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.ErrorLog)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Message</td>
                <td>@item.StackStrace</td>
                <td>@item.CretedDate</td>
            </tr>
        }

    </tbody>
</table>

